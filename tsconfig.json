{
  "compilerOptions": {
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "module": "esnext",
    "lib": [
      "dom",
      "DOM.Iterable",
      "esnext"
    ],
    "jsx": "react-jsx",
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "importHelpers": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "pretty": true,
    "esModuleInterop": true,
    "baseUrl": "./src/",
    "allowJs": true,
    "paths": {
      "@components/*": [
        "Components/",
        "Components/*"
      ],
      "@containers/*": [
        "Containers/",
        "Containers/*"
      ],
      "@pages/*": [
        "Pages/",
        "Pages/*"
      ],
      "@assets/*": [
        "assets/*",
        "assets/"
      ],
      "@stores/*": [
        "stores/",
        "stores/*"
      ],
      "@utils/*": [
        "utils/",
        "utils/*"
      ],
      "@hooks/*": [
        "hooks/",
        "hooks/*"
      ],
      "@app/*": [
        "app/",
        "app/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@api": [
        "api/index.ts"
      ],
      "@styles": [
        "styles/*",
        "assets/styles/"
      ],
    },
    "strictPropertyInitialization": false,
  },
  "include": [
    "./src/**/*",
    "src/containers/LectureQuiz/.tsx"
, "postcss.config.js"  ],
  "parserOptions": {
    "extraFileExtensions": [
      ".mjs"
    ]
  },
}
